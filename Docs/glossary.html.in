			</div>
			<div class="cell index_link primary_cell">
				<a href="./" rel="index"
					title="Up">&uarr;Back to Documentation Index</a>
			</div>
			<div class="cell next_link primary_cell">
				<a href="./" rel="next"
					title="Next">Documentation Index&rarr;</a>
			</div>
		</div>
@FOOTER@
@END@
